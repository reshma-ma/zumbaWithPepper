<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".feedbackActivity"
    android:background="@drawable/background"
    >

    <pl.droidsonroids.gif.GifImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/party"
        ></pl.droidsonroids.gif.GifImageView>



    <ImageView
        android:id="@+id/starOne"
        android:layout_width="wrap_content"
        android:layout_height="100dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="150dp"
        android:layout_marginTop="130dp"
        android:layout_marginEnd="400dp"
        android:src="@drawable/baseline_star_24" />
    <ImageView
        android:id="@+id/starTwo"
        android:layout_width="wrap_content"
        android:layout_height="130dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="400dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="400dp"
        android:src="@drawable/baseline_star_24" />
    <ImageView
        android:id="@+id/starThree"
        android:layout_width="wrap_content"
        android:layout_height="100dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="500dp"
        android:layout_marginTop="130dp"
        android:layout_marginEnd="280dp"
        android:src="@drawable/baseline_star_24" />

    <TextView
        android:id="@+id/appname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Level Completed"
        android:textSize="35dp"
        android:textColor="#FFF504"
        android:textStyle="bold"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="250dp"/>

    <ImageView
        android:id="@+id/replay"
        android:layout_width="211dp"
        android:layout_height="80dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="39dp"
        android:layout_marginTop="433dp"
        android:layout_marginEnd="713dp"
        android:src="@drawable/baseline_replay_circle_filled_24" />

    <ImageView
        android:id="@+id/again"
        android:layout_width="211dp"
        android:layout_height="80dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="727dp"
        android:layout_marginTop="435dp"
        android:layout_marginEnd="27dp"
        android:src="@drawable/baseline_play_circle_24" />


</RelativeLayout>